<Window x:Class="MoneyRules.UI.Windows.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Реєстрація" Height="300" Width="400">
    <Grid Margin="20">
        <StackPanel>
            <TextBox x:Name="NameBox" Margin="0,0,0,10"/>
            <TextBox x:Name="EmailBox" Margin="0,0,0,10"/>
            <PasswordBox x:Name="PasswordBox" Margin="0,0,0,10"/>
            <Button Content="Зареєструватися" Click="Register_Click" Height="35"/>
        </StackPanel>
    </Grid>
</Window>
